[submodule "build/ffmpeg"]
	path = build/ffmpeg
	url = https://git.ffmpeg.org/ffmpeg.git
	branch = release/5.1
[submodule "build/zlib"]
	path = build/zlib
	url = https://github.com/madler/zlib.git
	ignore = dirty
[submodule "build/emscripten"]
